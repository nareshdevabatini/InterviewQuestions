static void Main()
{
    int size=5;
		int temp=0;
		bool evenrow=false;
		int count=0;
		for(int i=0; i<size*size;i++)
		{
			if(i%size==0 && i!=0){
				Console.WriteLine("\n");
				if(evenrow){
					temp=size;
					count=i;
				}
			}
			if(temp==0){
				Console.Write(i+1	+"\t");
				evenrow=true;
			}
			else{
				temp--;
				Console.Write(count+1+temp+ "\t");
				evenrow=false;
				
			}
		}
 }
 
Ans:
1	  2	  3	  4	  5	

10	9	  8	  7	  6	

11	12	13	14	15	

20	19	18	17	16	

21	22	23	24	25
